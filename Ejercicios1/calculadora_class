class calculadora_class:
  def __init__(self):
        self.a=input('introduce un numero: ')
        self.b=input('introduce otro: ')
  def suma(self):
    print(f'El resultado de la suma', self.a+self.b)
    return self.__a+b
  def resta(self):
    print(f'El resultado de la resta', self.a-self.b)
    return self.__a-self.b
  def multiplicacion(self):
    print(f'El resultado de la multiplicacion', self.a*self.b)
    return self.__a*self.b
  def division(self):
    if(self.b==0):
        print('No se puede dividir entre 0')
        return
    else:
        print(f'El resultado de la division', self.a/self.b)
    return self.a/self.b
  if __name__=="__main__":
    
    print('Ejecutando como programa principal')
    division(10,0)